/**app.wxss**/

page {
  height: 100%;
  width: 100%;
}

.text-center {
  text-align: center !important;
}

.text-left {
  text-align: left !important;
}

.err {
  width: 100vw;
  background: #e64340;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  /* position: fixed;
   left: 0;
  top: 0;  */
  font-size: 30rpx;
  color: #fff;
  z-index: 999;
}

.float-left {
  float: left !important;
}

.float-right {
  float: right !important;
}

.clearfix {
  clear: both !important;
}

.d-inline-block {
  display: inline-block !important;
}

.clearfix::after {
  display: block;
  content: "";
  clear: both;
}

.btn-active {
  background: #11a2e4 !important;
  /* border: 1px solid rgba(5, 5, 5, 0.08)!important; */
  color: rgba(255, 255, 255, 0.5) !important;
}

.btn-primary {
  width: 670rpx;
  background: #28bcff;
  /* border: 1px solid rgba(5, 5, 5, 0.08); */
  margin: 0 auto;
  color: white;
}

.text-muted {
  color: #c1c1c1 !important;
}

input::-webkit-input-placeholder {
  font-size: 34rpx;
  color: #b2b2b2;
}

input:-moz-placeholder {
  font-size: 34rpx;
  color: #b2b2b2;
}

input::-moz-placeholder {
  font-size: 34rpx;
  color: #b2b2b2;
}

input:-ms-input-placeholder {
  font-size: 34rpx;
  color: #b2b2b2;
}

.text-warning {
  color: #f0b831 !important;
}

.bg-white {
  background-color: white !important;
}

.report-head {
  height: 104rpx;
  padding-top: 24rpx;
  box-sizing: border-box;
  white-space: nowrap;
  width: 100%;
  border-bottom: 1px solid #f4f4f4;
  background-color: rgba(255, 255, 255, 1);
  position: relative;
  /* position: fixed;
  left: 0;
  top: 0;
  z-index: 1000000000;  */
}

.report-head ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.report-item {
  background: #f3f3f3;
  border-radius: 200rpx;
  font-size: 30rpx;
  color: #9d9d9d;
  height: 56rpx;
  padding: 0 24rpx;
  line-height: 56rpx;
  display: inline-block;
  text-align: center;
  margin-left: 28rpx;
}

.report-item-end {
  margin-right: 28rpx;
}

.report-left {
  background-image: linear-gradient(-90deg, rgba(255,255,255,0.00) 0%, rgba(255,255,255,0.73) 45%, #FFFFFF 100%);
  width: 50rpx;
  height: 104rpx;
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.report-right {
  background-image: linear-gradient(-269deg, rgba(255,255,255,0.00) 0%, rgba(255,255,255,0.73) 45%, #FFFFFF 100%);
  width: 50rpx;
  height: 104rpx;
  position: fixed;
  right: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.report-left image, .report-right image {
  color: #faa403;
  width: 16rpx;
  height: 40rpx;
}

.report-item-active {
  color: #fcfcfc;
  background-image: linear-gradient(-131deg, #ffc530 3%, #ffbc25 100%);
  box-shadow: inset 0 0 5px 0 #ffcc5a;
}

.respondence-index {
  width: 64rpx;
  height: 52rpx;
  background: #28bcff;
  box-shadow: inset 0 0 10rpx 0 #5acbff;
  font-size: 30rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top-right-radius: 26rpx;
  border-bottom-right-radius: 26rpx;
}

.respondence-item-head {
  border-bottom: 1px solid #f4f4f4;
  height: 70rpx;
  display: flex;
  align-items: center;
  background: #fafafa;
  position: relative;
}

.respondence-item-head>view {
  padding-right: 31rpx;
  font-size: 26rpx;
  color: #9b9b9b;
  position: absolute;
  left: 84rpx;
  height: 66rpx;
  top: 0;
  line-height: 66rpx;
}

.respondence-item-head>view>text {
  margin-right: 14rpx;
}

.respondence-item-head>view .text-warning {
  color: #fa9003;
}

.no-data-info {
  font-size: 30rpx;
  color: #9b9b9b;
  height: 110rpx;
  line-height: 110rpx;
}

.child-list {
  width: 100%;
  height: 86rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  text-align: center;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  border: 1px solid #f4f4f4;
}

.child-list-item {
  background: rgba(255, 255, 255, 0.89);
  border: 1px solid #dadada;
  /* box-shadow:  0 0 9px 0 #fff; */
  width: 308rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: #9b9b9b;
  border-right-width: 0;
  position: relative;
  top: -2rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.child-list-item:first-child {
  border-bottom-left-radius: 100px;
  border-top-left-radius: 100px;
}

.child-list-item:last-child {
  border-bottom-right-radius: 100px;
  border-top-right-radius: 100px;
  border-right-width: 1px;
}

.childActive {
  background: #28bcff;
  border: 1px solid #28bcff;
  color: #fff;
}

.child-item-two {
  width: 25%;
}

.my-child-item {
  background-color: white;
  margin-bottom: 30rpx;
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
}

.child-item-head {
  margin-left: 40rpx;
  border-bottom: 1px solid #e5e5e5;
  height: 120rpx;
  display: flex;
  align-items: center;
  position: relative;
}

.child-item-head>text, .child-item-header-title {
  font-size: 36rpx !important;
  color: #000 !important;
  margin-right: 10rpx !important;
  position: relative !important;
  left: 0 !important;
}

.child-item-header-title>text {
  color: #f13939 !important;
}

.child-item-head>button {
  background: #ffc530;
  border-radius: 8rpx;
  min-width: 130rpx;
  height: 60rpx;
  position: absolute;
  right: 32rpx;
  top: 30rpx;
  font-size: 28rpx;
  color: #fff;
  line-height: 60rpx;
}

.child-item-head>button:active {
  background-color: #ecad0c;
  color: rgba(255, 255, 255, 0.6);
}

.child-item-head>button:after {
  border: none;
}

.child-item-head>image {
  width: 46rpx;
  height: 36rpx;
}

.child-item-head>view {
  font-size: 30rpx;
  color: #888;
  position: absolute;
  left: 236rpx;
}

.child-item-head>view text {
  color: #927542;
  margin-left: 6rpx;
}

.child-item-content {
  margin-left: 40rpx;
  padding-bottom: 52rpx;
}

.child-item-content view.clearfix {
  margin-top: 30rpx;
  padding-right: 40rpx;
}

.clild-item-border {
  border-bottom: 1px solid #e5e5e5;
  padding-bottom: 30rpx;
}

.child-item-content view.clearfix text:first-child {
  font-size: 32rpx;
  color: #3a3a3a;
}

.child-item-content view.clearfix text:last-child {
  font-size: 32rpx;
  color: #888;
}

.role-info {
  margin-top: 12rpx;
  background-color: white;
