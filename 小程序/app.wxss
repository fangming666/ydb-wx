  left: 0;
  top: 0;
  width: 54rpx;
  height: 50%;
  border-bottom: 1px solid #c8c8c8;
}

.open-service-info::before {
  content: "";
  display: block;
  position: absolute;
  right: 0;
  top: 0;
  width: 54rpx;
  height: 50%;
  border-bottom: 1px solid #c8c8c8;
}

.open-service-foot {
