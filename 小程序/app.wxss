  box-shadow: inset 0 0 5px 0 #ffcc5a;
  border-radius: 100px;
  font-size: 30rpx;
  color: #fcfcfc;
  width: 384rpx;
  height: 80rpx;
  margin-bottom: 40rpx;
}

.open-service-warp button:active {
  background-image: linear-gradient(-131deg, #fdb600 3%, #eea500 100%);
  box-shadow: inset 0 0 5px 0 #e5a517;
}

button::after {
  border: none;
}

.btn-sm {
