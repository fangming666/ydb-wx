/* pages/my-item/myDirectory /directory.wxss */

.directory-warp {
  min-height: 100vh;
  background-color: white;
}

.directory {
  padding: 28rpx 0 32rpx 36rpx;
}

.directory-name {
  font-size: 16px;
  color: #3a3a3a;
  margin-bottom: 24rpx;
}

.directory-nav {
  font-size: 32rpx;
  color: #0886dc;
  display: block;
  margin-bottom: 20rpx;
  padding-left: 62rpx;
  position: relative;
}

.directory-nav::after {
  content: "";
  display: block;
  width: 12rpx;
  height: 12rpx;
  background: #fff;
  border: 1px solid #0886dc;
  position: absolute;
  left: 25rpx;
  top:17rpx;
  border-radius: 50%;
}
