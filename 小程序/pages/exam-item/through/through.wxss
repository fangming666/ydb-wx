.through {
  min-height: 100vh;
  background: linear-gradient(0deg, #302dab 0%, #6361b4 100%);
  width: 100%;
  overflow: hidden;
}

.through-exam {
  background: #434494;
  border: 1px solid #5e71c1;
  font-size: 34rpx;
  box-shadow: 0 0 4px 0 #706eba, inset 0 1px 10px 0 rgba(35, 36, 133, 0.65);
  border-radius: 20rpx;
  width: 700rpx;
  line-height: 1.5;
  display: flex;
  box-sizing: border-box;
  justify-content: center;
  align-items: center;
  margin: 28rpx auto 12rpx;
  padding: 8px 10px;
  color: white;
  text-align: center;
}

.through-my {
  width: 100%;
  box-sizing: border-box;
  padding: 0 36rpx 26rpx;
   text-align: center; 
}

.through-my>view {
  font-size: 30rpx;
  color: #fff;
  display: inline-block;
}

.through-my>view:first-child {
  margin-right: 30rpx;
}



.through-select {
  width: 700rpx;
  display: flex;
  margin: 0 auto;
  position: relative;
  z-index: 2;
}

.through-select>view {
  flex: 1;
  height: 84rpx;
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/cy@3x.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: cadetblue;
}

.through-select>view.through-active {
  height: 118rpx;
}

.through-select>view:last-child {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/cjmn@3x.png");
}

.through-select>view:first-child.through-active {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/cy_click@3x.png");
}

.through-select>view:last-child.through-active {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/cjmn_click@3x.png");
}

.through-content {
  background: rgba(255, 255, 255, 0.18);
  border: 1px solid rgba(255, 255, 255, 0.61);
  box-shadow: inset 0 0 28px 0 rgba(255, 255, 255, 0.27);
  margin: -24px auto 0;
  width: 667rpx;
  position: relative;
  left: 5rpx;
}

.through-content>view {
  padding-top: 2.2rem;
  padding-bottom: 1rem;
}

picker {
  display: inline-block;
}

.through-class, .through-sub,.through-school {
  width: 80%;
  margin: 0 auto 80rpx;
}

.through-class>text, .through-sub>text,.through-school>text {
  font-size: 44rpx;
  color: #fff;
}

.through-class .picker>view, .through-sub .picker>view,.through-school .picker>view {
  width: 272rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}

.through-class .picker>view {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/choose_class@2x.png");
}

.through-sub .picker>view {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/choose_sub@2x.png");
}

.through-class .picker>view:active {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/choose_class_click@2x.png");
}

.through-sub .picker>view:active {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/choose_sub_click@2x.png");
}

.through-school .picker>view{
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/choose_school@2x.png")
}

.through-school .picker>view:active{
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/choose_school_click@2x.png")
}

.through-foot-img {
  height: 48rpx;
  width: 671rpx;
  position: relative;
  left: 5rpx;
  margin: 0 auto;
}

.through-btn {
  width: 628rpx;
  height: 148rpx;
  margin: 41rpx auto 0;
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% 100%;
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/start_cy@2x.png");
}

.through-btn:active {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/start_cy_click@2x.png");
}

.simulation-title {
  font-size: 40rpx;
  color: #fff;
  margin-left: 12rpx;
  text-align: left;
  margin-bottom: 16rpx;
  position: relative;
}

.simulation-title>image {
  width: 24rpx;
  height: 40rpx;
  margin-right: 0.4rem;
  position: relative;
  top: 3px;
}

.simulation-content>input {
  width: 320rpx;
  height: 110rpx;
  background: #fff;
  border: 1px solid #db524b;
  border-radius: 9px;
  margin-right: 24rpx;
  font-size: 56rpx;
  font-weight: 600
}

.simulation-now {
  padding-left: 25rpx;
  padding-right: 25rpx;
}

.simulation-btn {
  width: 222rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/jiSuan@2x.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  height: 110rpx;
  margin-bottom: 30rpx;
}

.simulation-btn:active {
  background-image: url("https://home.yuandingbang.cn/static/wechat/images/jiSuan_click@2x.png");
}

.simulation-btn>image {
  width: 120rpx;
  height: 59rpx;
}

.simulation-result {
  text-align: left;
  width: 580rpx;
  margin: 0 auto 60rpx;
  background: #3254c0;
  border: 1px solid #2140a6;
  border-radius: 4px;
  color: white;
  /* height: 154rpx; */
  box-sizing: border-box;
  padding: 18rpx 0 12rpx 20rpx;
}

.simulation-result>view:first-child {
  font-size: 32rpx;
}

.simulation-result>view:last-child {
  font-size: 36rpx;
  margin-top: 14rpx;
  display: inline-block
}

.simulation-result>view text {
  font-size: 64rpx;
}



.simulation-text {
  width: 667rpx;
  text-align: left;
  font-size: 30rpx;
 color: rgba(255,255,255,0.66);
  margin: 38rpx auto;
}

input {
  color: #fa9003;
}

.no-vip-title {
  font-size: 34rpx;
  color: #e2e2f9;
  margin: 68rpx auto 62rpx;
  text-align: center;
  width: 624rpx;
}

.no-vip-info {
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  margin-bottom: 54rpx;
}

.no-vip-btn {
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  height: 148rpx;
  width: 624rpx;
  margin: 0 auto;
  background-image: url("https://home.dev.mschool.cn/static/wechat/images/throught-no.svg");
}

.no-vip-btn:active {
  background-image: url("https://home.dev.mschool.cn/static/wechat/images/throught-no-click.svg");
}

.through-modal {
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.50);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
}

.through-modal-body image {
  width: 100%;
  height: 720rpx;
}

.through-modal-body {
  width: 100%;
  margin: 60rpx auto;
  position: relative;
}

.rate {
  width: 100%;
  text-align: center;
  position: absolute;
  top: 300rpx;
  left: 0;
  font-size: 120rpx;
  color: #ff9a16;
}

.rate-text {
  width: 80%;
  text-align: center;
  position: absolute;
  top: 520rpx;
  font-size: 36rpx;
  color: #55595c;
  left: 50%;
  transform: translate(-50%);
}

.through-modal-btn {
  border: 1px solid #fff;
  border-radius: 100px;
  width: 292rpx;
  height: 88rpx;
  display: flex;
  color: white;
  justify-content: center;
  align-items: center;
  margin: 100rpx auto;
}

.through-modal-btn:active {
  background-color: rgba(255, 255, 255, 0.50)
}
